{
  "name": "ring-charts",
  "version": "0.0.1",
  "main": "lib/commonjs/index.js",
  "types": "lib/typescript/index.d.ts",
  "module": "lib/module/index.js",
  "source": "src/components/index.tsx",
  "files": [
    "src/components",
    "lib"
  ],
  "directories": {
    "lib": "lib"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:commonjs": "babel src/components --out-dir lib/commonjs --extensions '.ts,.tsx' --source-maps",
    "build:module": "tsc src/components/*.tsx --module es2022 --target es5 --jsx 'react' --sourceMap --skipLibCheck --allowSyntheticDefaultImports --outDir lib/module --moduleResolution node",
    "build:typescript": "tsc src/components/*.tsx --declaration --emitDeclarationOnly --jsx 'react'  --skipLibCheck --allowSyntheticDefaultImports  --outDir lib/typescript"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "## Props",
  "dependencies": {
    "eslint": "^8.18.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-module-utils": "^2.7.3",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsx-a11y": "^6.6.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-scope": "^7.1.1",
    "eslint-utils": "^3.0.0",
    "eslint-visitor-keys": "^3.3.0",
    "eslint-webpack-plugin": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^5.0.1",
    "typescript": "^4.7.4"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tohsaka888/ring-charts.git"
  },
  "keywords": [
    "react",
    "ring-charts",
    "echarts"
  ],
  "author": "tohsaka888",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tohsaka888/ring-charts/issues"
